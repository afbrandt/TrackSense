<% provide(:title, "Track Sense") %>
<div class="main-content">
  <div class="tag-header">
    <div class="tag-header-column">
      <h2 class="tag-title">Tags</h2>
    </div>
  </div>
  <div class="tag-box-wrapper">
    <% @all_tags.each do |tag| %>
      <div class="tag-box">
        <div class="tag-label"><%= link_to tag.name, "tag/#{tag.name}", class: "tag-inner-label" %></div>
      </div>
    <% end %>
  </div>
</div>
